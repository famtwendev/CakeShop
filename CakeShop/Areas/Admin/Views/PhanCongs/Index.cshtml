@model IEnumerable<CakeShop.Data.PhanCong>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NgayPc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HieuLuc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaNvNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaPbNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NgayPc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HieuLuc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNvNavigation.MaNv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaPbNavigation.MaPb)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaPc">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaPc">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaPc">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
